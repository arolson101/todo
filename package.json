    "prern:macos": "cd macos && pod install",
    "rn:macos": "react-native run-macos",
    "rn:start": "react-native start",
    "rn:windows": "react-native run-windows",
    "serve:hono:bun": "bun --port 3001 --watch server/hono.ts",
    "serve:hono:node": "tsx watch --env-file=.env server/serve-node.ts",
    "start": "cd dist && cross-env NODE_ENV=production bun --env-file=../.env run serve-bun.js",
    "start:node": "cd dist && cross-env NODE_ENV=production node --env-file=../.env serve-node.js"
