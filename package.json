{
  "name": "todo",
  "type": "module",
  "module": "index.ts",
  "scripts": {
    "build": "concurrently \"bun build:server\" \"bun build:client\"",
    "build:client": "bunx --bun vite build",
    "build:server": "cross-env NODE_ENV=production bun run scripts/build.ts",
    "db:dump": "bun run ./scripts/dump.ts",
    "db:migrations": "bunx drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:seed": "bun run ./scripts/seed.ts",
    "db:studio": "drizzle-kit studio",
    "dev": "concurrently \"bun dev:client\" \"bun dev:server\"",
    "dev:client": "bunx --bun vite",
    "dev:server": "bun run --hot server/index.ts",
    "format": "prettier --write .",
    "start": "cd dist && cross-env NODE_ENV=production bun --env-file=../.env run index.js"
  },
