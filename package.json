{
  "name": "todo",
  "type": "module",
  "module": "index.ts",
  "scripts": {
    "build": "concurrently \"bun:build:*\"",
    "build:hono": "cross-env NODE_ENV=production bun run scripts/build.ts",
    "build:vite": "bunx --bun vite build",
    "db:dump": "bun run ./scripts/dump.ts",
    "db:migrations": "bunx drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:seed": "bun run ./scripts/seed.ts",
    "db:studio": "drizzle-kit studio",
    "dev": "concurrently \"bun:dev:*\"",
    "dev:hono": "bun run serve:hono:node",
    "dev:vite": "bunx --bun vite",
    "format": "prettier --write .",
    "serve:hono:bun": "bun --port 3001 --watch server/hono.ts",
    "serve:hono:node": "tsx watch --env-file=.env server/serve-node.ts",
    "start": "cd dist && cross-env NODE_ENV=production bun --env-file=../.env run serve-bun.js",
    "start:node": "cd dist && cross-env NODE_ENV=production node --env-file=../.env serve-node.js",
    "tauri": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:ios": "tauri ios dev"
  },
