{
  "name": "todo",
  "type": "module",
  "module": "index.ts",
  "scripts": {
    "build": "concurrently \"bun build:server\" \"bun build:client\"",
    "build:client": "bunx --bun vite build",
    "build:server": "cross-env NODE_ENV=production bun run scripts/build.ts",
    "dev": "concurrently \"bun dev:client\" \"bun dev:server\"",
    "dev:client": "bunx --bun vite",
    "dev:server": "bun run --hot server/index.ts",
    "format": "prettier --write .",
    "start": "cd dist && cross-env NODE_ENV=production bun run index.js"
  },
  "dependencies": {
    "@hono/trpc-server": "^0.3.2",
    "@hono/zod-validator": "^0.2.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@tanstack/react-router": "^1.48.1",
    "@trpc/client": "^11.0.0-rc.477",
    "@trpc/react-query": "11.0.0-rc.477",
    "@trpc/server": "^11.0.0-rc.477",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "hono": "^4.5.6",
    "lucide-react": "^0.428.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@tanstack/eslint-plugin-query": "^5.51.15",
    "@tanstack/router-devtools": "^1.48.1",
    "@tanstack/router-plugin": "^1.48.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bun": "latest",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
