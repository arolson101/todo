    "build": "concurrently \"bun:build:*\"",
    "build:hono": "cross-env NODE_ENV=production bun run scripts/build.ts",
    "build:vite": "vite build",
    "db:dump": "bun run ./scripts/dump.ts",
    "db:migrations": "concurrently \"bun:db:migrations:*\"",
    "db:migrations:client": "bunx drizzle-kit generate --config drizzle-app.config.ts",
    "db:migrations:server": "bunx drizzle-kit generate",
